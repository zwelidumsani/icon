	<section class="newsletter-section-two" style="background-image:url(images/background/6.jpg); margin-bottom:0px">

		<div class="auto-container">
			<div class="row clearfix">
				<!-- Title Column -->
				<div class="title-column col-lg-7 col-md-12 col-sm-12">
					<!-- Sec Title -->
					<div class="sec-title light">
						<div class="title-inner">
							<div class="title">Donâ€™t miss out on anything!</div>
							<h2>Subscribe to our newsletter</h2>
						</div>
					</div>
					<div class="text"> Get the latest news about ICON construction company.<br> Learn more about current projects.</div>
					
					<!-- Newsletter Form -->
					<div class="newsletter-form">
					     	{{#if TeamEmailSuccess}} 
							 <div class = "alert alert-success" alert-dismissable style = "border-radius:10px;>
								 <p class= "pull-right"><button type = "button" class = "close" data-dismiss = "alert" aria-hidden = "true"><i class = "lnr lnr-cross-circle"></i></button>
								 <i class = "lnr lnr-checkmark-circle"></i>  Email address was successfully sent.</p>
							 </div>	
							{{/if}}
			   
				             {{#if TeamEmailError}} 
						    <div class = "alert alert-danger" alert-dismissable style = "border-radius:10px;>
							 <p class= "pull-right"><button type = "button" class = "close" data-dismiss = "alert" aria-hidden = "true"><i class = "lnr lnr-cross-circle"></i></button>
							 <i class = "lnr lnr-cross-circle"></i>  Could not send email.</p>
						    </div>	
				            {{/if}}
							<form method="post" action="/email">
								<div class="form-group clearfix">
									<input type="email" name="email" value="" placeholder="Email Address" required>
									<button type="submit" class="theme-btn btn-style-four">Submit Now</button>
								</div>
							</form>
					</div>
					
				</div>
				<!-- Image Column -->
				<div class="image-column col-lg-5 col-md-12 col-sm-12">
					<div class="inner-column">
						<div class="image">
							<img src="images/resource/newsletter.png" alt="" />
						</div>
					</div>
				</div>
			</div>
